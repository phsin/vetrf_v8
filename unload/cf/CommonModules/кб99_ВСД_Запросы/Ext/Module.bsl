// Проверяет номер автомобиля на наличие латинских символов
Процедура ПроверитьНомерАвтомобиля(РезультатПроверки, Документ)
	
	Отказ = Ложь;
	РазрешенныеСимволы = "АВЕКМНОРСТУХ0123456789 ";
	НомерАвто = СокрЛП(Документ.НомерАвто);
	
	ЗапрещенныеСимволы = "";
	КодСтраны = ВРег(Прав(НомерАвто, 3));
	Если КодСтраны = "RUS" Тогда
		
		НомерБезКодаСтраны = Лев(НомерАвто, СтрДлина(НомерАвто)-СтрДлина(КодСтраны));
		
		Для сч = 1 По СтрДлина(НомерБезКодаСтраны) Цикл
			Если Найти(Врег(РазрешенныеСимволы), Врег(Сред(НомерБезКодаСтраны, Сч, 1))) = 0 Тогда
				ЗапрещенныеСимволы = ЗапрещенныеСимволы + Сред(НомерБезКодаСтраны, Сч, 1);
			КонецЕсли;
		КонецЦикла;
		
		Если ЗначениеЗаполнено(ЗапрещенныеСимволы) Тогда
			Отказ = Истина;
			стрОшибка = "Номер автомобиля содержит запрещенные символы: "+ЗапрещенныеСимволы;	
		КонецЕсли;
	Иначе
		Отказ = Истина;
		стрОшибка = "В номере автомобиля указан не известный код страны: "+ КодСтраны;
	КонецЕсли;
	
	Если Отказ Тогда
		РезультатПроверки = Ложь;
		кб99_ВСД.СообщитьИнфо(СтрШаблон("В документе %1 %2", Документ.Ссылка, стрОшибка));
		Документ.Комментарий = ?(ЗначениеЗаполнено(Документ.Комментарий), Документ.Комментарий + ", " + стрОшибка, стрОшибка);
	КонецЕсли;
	
КонецПроцедуры

