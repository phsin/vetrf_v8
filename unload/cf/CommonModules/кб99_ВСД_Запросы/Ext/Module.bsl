			ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		
		ОтменитьТранзакцию();		
		Ответ = Ложь;
		СообщитьИнфо("Ошибка при получении ВСД_Улицы "+ ОписаниеОшибки());
	КонецПопытки;
			
	Возврат Ответ;
	
