			Запрос = Новый Запрос("ВЫБРАТЬ
			                      |	ВСД_Район.Ссылка КАК Ссылка,
			                      |	ВСД_Район.Владелец КАК Владелец
			                      |ИЗ
			                      |	Справочник.ВСД_Район КАК ВСД_Район
			                      |ГДЕ
			                      |	ВСД_Район.GUID = &GUID");
			Запрос.УстановитьПараметр("GUID", guid );
			
			Результат = Запрос.Выполнить().Выбрать();
			
			Если Результат.Следующий() Тогда 
				Если НЕ ЗначениеЗаполнено(Результат.Владелец) Тогда
					Объект = Результат.Ссылка.ПолучитьОбъект();
