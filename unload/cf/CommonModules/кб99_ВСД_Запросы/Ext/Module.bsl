    Service = "platform/services/2.1/IkarService";
    Action = "GetLocalityListByLocality";

	ПараметрыОтправки = кб99_ВСД_Отправка.ПараметрыОтправкиИнициализация( Параметры );
	ПараметрыОтправки.ЗапросXML = ЗапросXML;
    ПараметрыОтправки.Service = Service;
    ПараметрыОтправки.Action = Action;	
	xdto = кб99_ВСД_Отправка.ОтправитьSOAPНаСервере( ПараметрыОтправки );
	
	Если НайтиОшибки(xdto, Город ) Тогда
		Возврат Ложь;
	КонецЕсли;

	НачатьТранзакцию();
	Попытка 
		Если Число(xdto.Body.getLocalityListByLocalityResponse.localityList.count) = 0 Тогда
			ТекстСообщения = СтрШаблон("По городу %1 в классификаторе отсутствуют прикрепленные населенные пункты", Город.Наименование);
			СообщитьИнфо(ТекстСообщения);
			Возврат Истина;
	   	КонецЕсли;
		
		LocalityList = xdto.Body.getLocalityListByLocalityResponse.LocalityList.Locality;
		Если ТипЗнч(LocalityList) = Тип("ОбъектXDTO") Тогда
			ДобавитьНаселенныйПункт(LocalityList, Город);
		Иначе
			Для каждого Locality ИЗ LocalityList Цикл		
				ДобавитьНаселенныйПункт(Locality, Город);		
			КонецЦикла;
		КонецЕсли;
	
		ЗафиксироватьТранзакцию();
		Ответ = Истина;
	    СообщитьИнфо("Загрузка населенных пунктов завершена");
