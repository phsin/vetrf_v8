	Если НайтиОшибки(xdto, _Регион ) Тогда
		Возврат Ложь;
	КонецЕсли;

	НачатьТранзакцию();
	Попытка
		LocalityList = xdto.Body.getLocalityListByRegionResponse.LocalityList.Locality;
		Для каждого Locality ИЗ LocalityList Цикл		
				
			guid = Locality.guid;
			_uuid = Locality.uuid;
			name = Locality.view;			
			
