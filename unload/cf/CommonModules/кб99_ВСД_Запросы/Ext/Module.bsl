	|<bs:guid>"+СокрЛП(ДокСсылка.Отправитель_ХозСубъект.GUID)+"</bs:guid>
	|</dt:businessEntity>
	|</dt:businessEntity>";
	Если ЗначениеЗаполнено(ДокСсылка.Отправитель_Площадка) Тогда // У импортных ВСД отсутствует
