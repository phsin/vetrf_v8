		ЗаписьЖурналаРегистрации(
			НСтр("ru = 'Ошибка при получении населенных пунктов'"),
			УровеньЖурналаРегистрации.Ошибка,,,
			ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
		
		ОтменитьТранзакцию();		
		Ответ = Ложь;
		СообщитьИнфо("Ошибка при получении населенных пунктов: "+ ОписаниеОшибки());
