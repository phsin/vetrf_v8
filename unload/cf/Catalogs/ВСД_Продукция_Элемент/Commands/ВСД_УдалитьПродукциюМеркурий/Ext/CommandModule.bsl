	СтруктураКоманды.Вставить("ТекстСостояния", ТекстСостояния);
	
	кб99_Команды_Общий_Клиент.кб99_ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды, СтруктураКоманды);
	
